{
  "compilerOptions": {
    "rootDir": "../",
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "noUnusedParameters": true
  },
  "include": [
    "../src/vscode/extension.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}